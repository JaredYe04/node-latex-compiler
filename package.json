{
  "name": "node-latex-compiler",
  "version": "1.0.0",
  "tectonicVersion": "0.14.1",
  "description": "Node.js Tectonic LaTeX compiler with automatic binary download. Supports multiple input/output formats (file, text, buffer)",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/JaredYe04/node-latex-compiler.git"
  },
  "engines": {
    "node": ">= 14.x"
  },
  "keywords": [
    "latex",
    "tectonic",
    "tex",
    "pdf",
    "compiler",
    "latex-compiler",
    "document-compiler"
  ],
  "optionalDependencies": {
    "@node-latex-compiler/bin-win32-x64": "^1.0.0",
    "@node-latex-compiler/bin-darwin-x64": "^1.0.0",
    "@node-latex-compiler/bin-darwin-arm64": "^1.0.0",
    "@node-latex-compiler/bin-linux-x64": "^1.0.0"
  },
  "scripts": {
    "test": "node scripts/test-compilation.js",
    "test:local": "node scripts/test-compilation.js",
    "download": "node scripts/download-tectonic.js",
    "prepublish": "node scripts/download-tectonic.js --latest || echo 'Binary download failed, continuing...'"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib/",
    "scripts/",
    "README.md",
    "LICENSE"
  ],
  "author": "",
  "license": "MIT"
}
